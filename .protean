  cargo clean
  trivy image "localhost:5000/mihno:test" > ../mihno_trivy-report_$(date +%Y%m%d%H%M%S).txt
  docker-compose up -d
  curl localhost:3975 | grep "7e65002f3d5bae04afc4f0b66c14e4bed56680e2ffae3b86f4209e607cffdadb" || exit 1
